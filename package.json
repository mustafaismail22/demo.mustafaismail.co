{
  "name": "demo.mustafaismail.co",
  "description": "",
  "version": "1.0.0",
  "author": "Mustafa ismail",
  "license": "ISC",
  "main": "./index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "npm run clean && better-npm-run build",
    "dev": "npm run clean && concurrently --kill-others \"npm run dev:watch\" \"npm run dev:server\"",
    "dev:watch": "better-npm-run dev:watch",
    "dev:server": "better-npm-run dev:server",
    "clean": "rimraf dist/ webpack-assets.json",
    "lint": "eslint ./index.js ./src ./webpack; exit 0"
  },
  "betterScripts": {
    "start": {
      "command": "node ./index.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack -p --progress --colors --display-error-details --display-reasons --config ./webpack/prod.config.babel.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev:watch": {
      "command": "webpack --watch --progress --colors --display-error-details --display-reasons --config ./webpack/dev.config.babel.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:server": {
      "command": "node ./index.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "history": "^3.0.0",
    "lru-cache": "^4.0.1",
    "morgan": "^1.7.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-helmet": "^3.1.0",
    "react-slick": "^0.13.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.2",
    "slick-carousel": "^1.6.0",
    "webpack-isomorphic-tools": "^2.5.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-react": "^6.0.15",
    "better-npm-run": "0.0.11",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.8.2",
    "node-sass": "^3.4.2",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^0.10.0",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.3.3",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
